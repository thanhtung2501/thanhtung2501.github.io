<!DOCTYPE html>

<html>
    <head>
        <meta charset="utf-8" />
    </head>
    <body>
        <form action="/addToCart" method="post">
            <table>
                <tr>
                    <th>Name</th>
                    <th>Price</th>
                    <th>Quantity</th>
                </tr>
                <%for (const p of products) {%>
                    <tr>
                        <td><input type="text" readonly name="name" value="<%=p.getName()%>"/></td>
                        <td><input type="text" readonly name="price" value="<%=p.getPrice()%>"/></td>
                        <td><input type="text" name="quantity" value=""/></td>
                        <td>
                            <input type="submit" value="Add To Cart"/>
                        </td>
                    </tr>
                <%}%>
            </table>
        </form>
    </body>
</html>